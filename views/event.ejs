<!DOCTYPE html>
<html>
<head>
    <%= include templates/head %>
</head>
<body>
    <%= include templates/nav %>

    <article class="event-article">
        <h1 class="title"><%= title %></h1>
        <p class="times"><span class="event-start"><%= start %></span> - <span class="event-end"><%= end %></span></p>
        <img src="<%= img %>" alt="" />
        <p class="description"><%= description %></p>

        <hr />

        <div class="content">
            <%- html %>
        </div>
    </article>

<%= include templates/footer %>
</body>
</html>
